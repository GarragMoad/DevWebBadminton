{% extends 'base.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ club.nom }}</td>
            </tr>
            <tr>
                <th>Sigle</th>
                <td>{{ club.sigle }}</td>
            </tr>
            <tr>
                <th>Gymnase</th>
                <td>{{ club.gymnase }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ club.adresse }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_club_index') }}">back to list</a>

    <a href="{{ path('app_club_edit', {'id': club.id}) }}">edit</a>

    {{ include('club/_delete_form.html.twig') }}
{% endblock %}
